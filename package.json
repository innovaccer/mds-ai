{
  "name": "mds-ai",
  "version": "1.0.0",
  "description": "AI components library project for Innovaccer Design System",
  "homepage": "https://innovaccer.github.io/mds-ai",
  "main": "dist/bundle.umd.js",
  "module": "dist/bundle.esm.js",
  "types": "dist/src/common.type.d.ts",
  "directories": {
    "css": "css"
  },
  "scripts": {
    "test": "jest",
    "storybook": "storybook dev -p 5001",
    "build-storybook": "storybook build",
    "dev": "npm run storybook",
    "build-js": "rollup -c",
    "build": "npm run build-js",
    "pre-deploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "precommit": "lint-staged",
    "lint:check": "eslint ./src --ext .tsx,.ts,.js,.jsx",
    "lint": "npm run lint:check -- --fix",
    "prettier": "prettier --config ./.prettierrc --write 'src/components/**/*.css'"
  },
  "keywords": [],
  "author": "Innovaccer",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@figma/code-connect": "^0.1.1",
    "classnames": "^2.5.1",
    "react": "^16.12.0",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-strict-mode": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-react": "^7.24.1",
    "@chromatic-com/storybook": "^1.3.3",
    "@innovaccer/design-system": "^2.32.1",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-storysource": "^8.1.1",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.2",
    "@storybook/blocks": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-webpack5": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gh-pages": "^6.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.9",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "4.9.5",
    "typescript-eslint-parser": "^22.0.0"
  },
  "lint-staged": {
    "src/**/*.{tx,tsx,js,jsx}": [
